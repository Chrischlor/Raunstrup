@model Contact
@using Microsoft.AspNetCore.Mvc.RazorPages



<h1>Kontakt</h1>
<h2>@ViewData["title"]</h2>
@*<h3>@Model.PostedMessage</h3>*@
<div class="row">
    <div class="col-md-6">
        <form method="post">
            <div class="form-group">
                <label asp-for="Navn" class="control-label">Navn</label>
                <input asp-for="Navn" class="control-label" />
            </div>
            <div class="form-group">
                <label asp-for="Mail" class="control-label">Email</label>
                <input asp-for="Mail" class="control-label" />
            </div>
            <div class="form-group">
                <label asp-for="Emne" class="control-label">Emne</label>
                <input asp-for="Emne" class="control-label" />
            </div>
            <div class="form-group">
                <label asp-for="Besked" class="control-label">Besked</label>
                <input asp-for="Besked" class="control-label" />
            </div>
            <button type="submit" value="submit">Send Besked</button>
        </form>
    </div>

</div>


